{
  "name": "baks-dev/payment",
  "description": "Модуль способов оплаты",
  "homepage": "https://github.com/baks-dev/payment",
  "keywords": [
    "symfony",
    "bundle",
    "library",
    "module",
    "plugin",
    "payment",
    "baksdev"
  ],
  "license": "MIT",
  "type": "symfony-bundle",
  "authors": [
    {
      "name": "Baks Development",
      "email": "admin@baks.dev"
    }
  ],
  "require": {
    "php": ">=8.1",
    "symfony/uid": "^6.2",
    "symfony/security-bundle": "^6.2",
    "doctrine/doctrine-bundle": "^2.8",
    "baks-dev/core": "^6.2"
  },
  "require-dev": {
    "roave/security-advisories": "dev-latest",
    "symfony/test-pack": "^1.0"
  },
  "autoload": {
    "psr-4": {
      "BaksDev\\Payment\\": ""
    }
  },
  "autoload-dev": {
    "psr-4": {
      "BaksDev\\Payment\\Tests\\": "Tests/"
    }
  },
  "config": {
    "sort-packages": true,
    "allow-plugins": {
      "symfony/flex": true
    }
  }
}
